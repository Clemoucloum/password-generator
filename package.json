{
    "author": "Sebastien Rousseau <hello@password-generator.pro> (https://github.com/sebastienrousseau/password-generator)",
    "autoupdate": {
        "fileMap": [
            {
                "basePath": "dist",
                "files": [
                    "**/*"
                ]
            }
        ],
        "source": "git",
        "target": "git://github.com/sebastienrousseau/password-generator.git"
    },
    "bin": {
        "password-generator": "src/bin/index.js"
    },
    "bugs": {
        "url": "https://github.com/sebastienrousseau/password-generator/issues"
    },
    "description": "A fast, simple and powerful utility library for generating unique passwords to streamline your digital and mobile web development needs.",
    "devDependencies": {
        "@babel/node": "^7.16.8",
        "@babel/preset-env": "^7.16.11",
        "babel-jest": "^28.0.0-alpha.8",
        "chai": "^4.3.6",
        "commander": "^9.1.0",
        "filesizes": "^0.1.2",
        "jest": "^28.0.0-alpha.8",
        "jshint": "^2.13.4",
        "mkdirp": "^1.0.4",
        "mocha": "^9.2.2",
        "nodemon": "^2.0.15",
        "rimraf": "^3.0.2"
    },
    "directories": {
        "dictionaries": "dictionaries",
        "images": "images",
        "lib": "lib",
        "test": "test",
        "utils": "utils"
    },
    "engines": {
        "node": ">=13"
    },
    "filename": "src/dist/bin/password-generator.js",
    "homepage": "https://password-generator.pro",
    "keywords": [
        "base64",
        "complex",
        "crypto",
        "cryptography",
        "dictionary",
        "entropy",
        "encryption",
        "memorable",
        "mit-license",
        "open-source",
        "openssl",
        "pass",
        "passgen",
        "passphrase",
        "password generator",
        "password-generator",
        "preprocessor",
        "random",
        "secure",
        "security"
    ],
    "license": "MIT",
    "license_URI": "http://www.opensource.org/licenses/mit-license.php",
    "main": "src/bin/password-generator.js",
    "name": "@sebastienrousseau/password-generator",
    "publishConfig": {
        "access": "public",
        "registry": "https://npm.pkg.github.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sebastienrousseau/password-generator.git"
    },
    "scripts": {
        "build": "npm run dev:generate:password:generator && cat ./package.json | grep -v '\"private\":' > dist/package.json && npm pack ./dist",
        "dev:clean:directory": "rimraf \"dist/\"",
        "dev:copy:bin": "cp -R ./bin dist/bin",
        "dev:copy:dictionaries": "cp -R ./dictionaries dist/dictionaries/",
        "dev:copy:images": "cp -R images/ dist/images/",
        "dev:copy:lib": "cp -R ./lib dist/",
        "dev:copy:readme": "cp README.md dist/",
        "dev:copy:test": "cp -R ./lib test/",
        "dev:create:distribution": "mkdirp ./dist",
        "dev:filesizes:package": "filesizes dist/ > dist/package-report.txt",
        "dev:generate:password:generator": "npm run dev:clean:directory && npm run dev:create:distribution && npm run dev:copy:bin && npm run dev:copy:dictionaries && npm run dev:copy:images && npm run dev:copy:lib && npm run dev:copy:readme && npm run dev:copy:test && npm run dev:filesizes:package",
        "npm login": "npm publish $(node -p \"p=require('./package.json');p.name+'-'+p.version+'.tgz'\")",
        "password-generator": "node src/bin/index.js",
        "postpublish": "npm run publish-npm",
        "prepare:password:generator": "npm run build",
        "publish-npm": "npm publish --access public --ignore-scripts --@OWNER:registry='https://registry.npmjs.org'",
        "test": "jest"
    },
    "type": "module",
    "version": "1.0.4"
}