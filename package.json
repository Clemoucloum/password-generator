{
    "name": "password-generator",
    "version": "1.0.0",
    "description": "Password Generator - A fast, simple and powerful Yeoman Static Site, Blog and GitHub Pages generator designed for Jekyll",
    "license": "MIT",
    "main": "bin/password-generator.js",
    "repository": "sebastienrousseau/password-generator",
    "author": {
        "name": "Sebastien Rousseau",
        "email": "hello@password-generator.pro",
        "url": "https://github.com/sebastienrousseau/password-generator"
    },
    "engines": {
        "node": ">=12.22.0"
    },
    "settings": {
        "node": {
            "tryExtensions": [
                ".js",
                ".json",
                ".ts"
            ],
            "convertPath": {
                "src/**/*.ts": [
                    "^src/(.+?)\\.ts$",
                    "dist/$1.js"
                ]
            }
        }
    },
    "scripts": {
        "test": "mocha"
    },
    "files": [
        "app"
    ],
    "type": "module",
    "bugs": {
        "url": "https://github.com/sebastienrousseau/password-generator/issues"
    },
    "keywords": [
        "password-generator"
    ],
    "dependencies": {
        "chalk": "^5.0.1",
        "commander": "^9.1.0",
        "jshint": "^2.13.4"
    },
    "devDependencies": {
      "eslint-plugin-node": "^11.1.0"
  }
}
